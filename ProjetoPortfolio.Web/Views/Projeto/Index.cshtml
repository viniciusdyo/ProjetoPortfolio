@model IEnumerable<ProjetoPortfolio.Web.Models.ProjetoViewModel>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Index";
}

<h1>Projetos</h1>

<p>
    Adicionar novo Projeto
</p>

<div class="row">
    @foreach (var item in Model)
    {
        <div>
            <h3>@Html.DisplayFor(projetos => item.Titulo)</h3>
        </div>
        <div>
            <h3>@Html.DisplayFor(projetos => item.Descricao)</h3>
        </div>
        <div>
            <h3>@Html.DisplayFor(projetos => item.Status)</h3>
        </div>
        <div>
            <h3>@Html.DisplayFor(projetos => item.Excluido)</h3>
        </div>
    }
</div>
